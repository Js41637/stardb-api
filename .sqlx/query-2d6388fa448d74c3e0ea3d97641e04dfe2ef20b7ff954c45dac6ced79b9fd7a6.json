{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            *\n        FROM\n            (\n                SELECT\n                    RANK() OVER (ORDER BY damage DESC) global_rank,\n                    RANK() OVER (PARTITION BY region ORDER BY damage DESC) regional_rank,\n                    *\n                FROM\n                    scores_damage\n                NATURAL JOIN\n                    scores\n                WHERE\n                    ($2::TEXT IS NULL OR character = $2)\n                AND\n                    ($3::BOOLEAN IS NULL OR support = $3)\n            ) ranked\n        WHERE\n            uid = $1\n        ORDER BY\n            global_rank\n        LIMIT\n            1\n        ",
