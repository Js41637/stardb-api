  "query": "\n        SELECT\n            warp_lc_light_cones.*,\n            light_cones_text.name name\n        FROM\n            warp_lc_light_cones\n        LEFT JOIN\n            light_cones_text\n        ON\n            light_cones_text.id = light_cone AND light_cones_text.language = $3\n        WHERE\n            warp_lc_light_cones.id = $1\n        AND\n            uid = $2\n        ",
