  "query": "\n        SELECT\n            warp_lc_characters.*,\n            characters_text.name\n        FROM\n            warp_lc_characters\n        INNER JOIN\n            characters_text\n        ON\n            characters_text.id = character AND characters_text.language = $2\n        WHERE\n            uid = $1\n        ORDER BY\n            timestamp\n        ",
