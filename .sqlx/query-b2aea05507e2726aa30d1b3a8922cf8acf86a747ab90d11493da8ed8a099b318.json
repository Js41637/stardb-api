{
  "db_name": "PostgreSQL",
  "query": "SELECT\n            achievements.*,\n            percent,\n            series.name series_name,\n            series.priority series_priority\n        FROM\n            achievements\n        NATURAL LEFT JOIN\n            (SELECT id, (COUNT(*)::float) / (SELECT COUNT(DISTINCT username) from completed) percent FROM completed GROUP BY id) percents\n        INNER JOIN\n            series\n        ON\n            series_id = series.id\n        WHERE\n            achievements.id = $1\n        ",
