  "query": "\n        SELECT\n            *\n        FROM\n            (\n                SELECT\n                    RANK() OVER (ORDER BY achievement_count DESC, timestamp) global_rank,\n                    RANK() OVER (PARTITION BY region ORDER BY achievement_count DESC, timestamp) regional_rank,\n                    *\n                FROM\n                    scores\n            ) ranked\n        WHERE\n            uid = $1\n        ",
