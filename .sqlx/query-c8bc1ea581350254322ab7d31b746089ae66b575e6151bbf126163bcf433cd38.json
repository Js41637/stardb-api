{
  "db_name": "PostgreSQL",
  "query": "SELECT\n            achievements.*,\n            achievements_text.name,\n            achievements_text.description,\n            COALESCE(achievements_percent.percent, 0) percent,\n            series_text.name series_name\n        FROM\n            achievements\n        NATURAL LEFT JOIN\n            achievements_percent\n        INNER JOIN\n            achievements_text\n        ON\n            achievements.id = achievements_text.id AND achievements_text.language = $2\n        INNER JOIN\n            series\n        ON\n            series = series.id\n        INNER JOIN\n            series_text\n        ON\n            series = series_text.id AND series_text.language = $2\n        WHERE\n            achievements.id = $1\n        ",
