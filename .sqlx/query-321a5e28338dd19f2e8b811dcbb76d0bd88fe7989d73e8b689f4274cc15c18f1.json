  "query": "\n        SELECT\n            warp_special_characters.*,\n            characters_text.name\n        FROM\n            warp_special_characters\n        INNER JOIN\n            characters_text\n        ON\n            characters_text.id = character AND characters_text.language = $3\n        WHERE\n            warp_special_characters.id = $1\n        AND\n            uid = $2\n        ",
